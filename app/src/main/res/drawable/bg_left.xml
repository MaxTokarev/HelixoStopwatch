<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <item
        android:bottom="4dp"
        android:right="4dp"
        android:start="4dp"
        android:top="4dp"
        tools:targetApi="m">
        <shape android:shape="oval">
            <solid android:color="@color/reset" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/reset" />
        </shape>
    </item>
</layer-list>